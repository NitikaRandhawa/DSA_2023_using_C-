#include <iostream>
using namespace std;



int main()
{
    int n;
    int arr[n];
    cout<<"enter size of array=";
    cin>>n;
    for( int i = 0; i< n; i++){
      cin>>arr[i];
      
    }
    cout<<"Array elements="<<endl;
    for(int i = 0;i< n; i++){
      cout<<arr[i]<<endl;
    }
    cout<<"Array Elements in reverse order:-"<<endl;
    for(int i = n-1; i>=0;i--){
      cout<<arr[i]<<endl;
    }
    return 0;
}
