#include <iostream>
using namespace std;


int main()
{
    int n;
    int arr[n];
    cout<<"enter size of array=";
    cin>>n;
    for( int i = 0; i< n; i++){
      cin>>arr[i];
      
    }
    cout<<"Array elements="<<endl;
    for(int i = 0;i< n; i++){
      cout<<arr[i]<<endl;
    }
    int min = arr[0];
    for(int i = 0;i<n;i++){
      if (arr[i]<min){
        min = arr[i];
      }}
      cout<<"Smallest element="<<min<<endl;
    
    int max = arr[0];
    for( int i = 0;i<n;i++){
      if(arr[i]>max){
       max = arr[i];
       }}
        cout<<"Largest element="<<max<<endl;
      
    return 0;
}
